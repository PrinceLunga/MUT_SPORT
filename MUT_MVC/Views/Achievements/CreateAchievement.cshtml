@model MUT_MODELS.AchievementModel

@{
    ViewData["Title"] = "CreateAchievement";
    Layout = "~/Views/Shared/Student_Layout.cshtml";
}

<h1>CreateAchievement</h1>

<h4>AchievementModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateAchievement">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AchievementDescription" class="control-label"></label>
                <input asp-for="AchievementDescription" class="form-control" />
                <span asp-validation-for="AchievementDescription" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateAchieved" class="control-label"></label>
                <input asp-for="DateAchieved" class="form-control" />
                <span asp-validation-for="DateAchieved" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

